{
  "name": "solana-memecoin-api",
  "version": "1.0.0",
  "description": "REST API for fetching Solana memecoin details from multiple sources",
  "main": "dist/main.js",
  "scripts": {
    "cli": "bun run src/cli.ts",
    "build": "bun build src/main.ts --outdir dist",
    "start": "bun run src/main.ts",
    "tsc:build": "tsc -p tsconfig.tsc.json",
    "tsc:start": "node dist/main.js",
    "start:dev": "bun x nodemon",
    "start:debug": "bun x nodemon --config nodemon-debug.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "solana",
    "memecoin",
    "crypto",
    "api",
    "nestjs"
  ],
  "author": "Gaurav Vishwakarma",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@types/inquirer": "^9.0.9",
    "axios": "^1.6.0",
    "chalk": "^5.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "inquirer": "^12.9.2",
    "ora": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}